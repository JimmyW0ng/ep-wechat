<!--pages/course/evaluatePage/EvaluatePage.wxml-->
<view class='page'>
  <view>分数{{score}}</view>

  <score-star-large score="30" bind:change="changeScore"></score-star-large> 

  <image class="uploaded-img" src="{{previewImg}}"></image>

  <img-uploader bind:change="changeImg">上传图片</img-uploader>  

  <avatar-selector list="{{avatarList}}"></avatar-selector>
</view>
