/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}

@font-face {
  font-family: 'iconfont'; /* project id 561892 */
  src: url('//at.alicdn.com/t/font_561892_fy48wnup2s4f5hfr.eot');
  src: url('//at.alicdn.com/t/font_561892_fy48wnup2s4f5hfr.eot?#iefix') format('embedded-opentype'),
         url('//at.alicdn.com/t/font_561892_fy48wnup2s4f5hfr.woff') format('woff'),
         url('//at.alicdn.com/t/font_561892_fy48wnup2s4f5hfr.ttf') format('truetype'),
         url('//at.alicdn.com/t/font_561892_fy48wnup2s4f5hfr.svg#iconfont') format('svg');
